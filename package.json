{
  "name": "my-workspace",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "mobile": "nx start rental-mobile",
    "mobile:ios": "nx run-ios rental-mobile",
    "mobile:android": "nx run-android rental-mobile",
    "mobile:web": "nx serve rental-mobile",
    "gateway": "nx serve api-gateway",
    "auth": "nx serve auth-service",
    "vehicle": "nx serve vehicle-service",
    "booking": "nx serve booking-service",
    "payment": "nx serve payment-service",
    "notification": "nx serve notification-service",
    "location": "nx serve location-service",
    "review": "nx serve review-service",
    "file-upload": "nx serve file-upload-service",
    "dev:core": "nx run-many --target=serve --projects=api-gateway,auth-service,vehicle-service,booking-service,payment-service,notification-service --parallel",
    "dev:backend": "nx run-many --target=serve --projects=api-gateway,auth-service,vehicle-service,booking-service,payment-service,notification-service,location-service,review-service,file-upload-service --parallel",
    "dev:services": "nx run-many --target=serve --projects=auth-service,vehicle-service,booking-service,payment-service,notification-service,location-service,review-service,file-upload-service --parallel",
    "build:all": "nx run-many --target=build --all",
    "test:all": "nx run-many --target=test --all",
    "generate": "node scripts/generate-prisma.js"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/runtime": "~7.27.6",
    "@expo/cli": "~0.24.14",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/js": "21.5.3",
    "@nx/node": "21.5.3",
    "@nx/web": "21.5.3",
    "@nx/webpack": "21.5.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "20.19.9",
    "@types/react": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "~13.2.0",
    "metro-config": "~0.82.4",
    "metro-resolver": "~0.82.4",
    "nx": "21.5.3",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@expo/metro-config": "^54.0.5",
    "@expo/metro-runtime": "^6.1.2",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nx/expo": "^21.5.3",
    "@nx/nest": "^21.5.3",
    "@rental-app/shared-prisma": "workspace:*",
    "@rental-app/shared-types": "workspace:*",
    "axios": "^1.12.2",
    "expo": "^54.0.10",
    "expo-splash-screen": "^31.0.10",
    "expo-status-bar": "^3.0.8",
    "expo-system-ui": "^6.0.7",
    "react": "^19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.64.0",
    "react-native": "^0.81.4",
    "react-native-svg": "^15.12.1",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-web": "^0.21.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  }
}
