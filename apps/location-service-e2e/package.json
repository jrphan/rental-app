{
  "name": "location-service-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "location-service"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/location-service-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "location-service:build",
          "location-service:serve"
        ]
      }
    }
  }
}
